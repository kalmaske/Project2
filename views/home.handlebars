    <div class="row">
        <form class="col s12">
            <div class="row">
                <div class="input-field col s4">
                    <input id="full_name" type="text" class="validate">
                    <label for="full_name">Full Name</label>
                </div>
                <div class="input-field col s4">
                    <input id="zip_code" type="text" class="validate">
                    <label for="zip_code">Zip Code</label>
                </div>
                <div class="input-field col s4" id="roomType">
                    <select>
                        <option value="" disabled selected>Choose your option</option>
                        <option value="Entire Home/Apt">Entire Home/Apt</option>
                        <option value="Private room">Private Room</option>
                        <option value="Shared room">Shared Room</option>
                    </select>
                </div>
            </div>
            <div class="row">
                <div class="input-field col s4">
                    <input id="price" type="text" class="validate">
                    <label for="price">Price</label>
                </div>
                <div class="input-field col s4">
                    <input id="min_night" type="text" class="validate">
                    <label for="email">Minimum Night Stay</label>
                </div>
                <div class="input-field col s4">
                    <input id="availability" type="text" class="validate">
                    <label for="availability">Availability</label>
                </div>
            </div>
        </form>
        <form>
            <button class="btn waves-effect" type="submit" name="action">Submit</button>
        </form>
    </div>
        </div>
        <div class="row">
            <br/>
            <input id="pac-input" class="controls" type="text" placeholder="Search Box">

            <div id="map">
                {{{this.map}}}
            </div>


        </div>
    <footer class="page-footer">
